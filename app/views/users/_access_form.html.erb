<%= form_for @user, url: login_path do |f| %>
    <%= f.text_field :name, placeholder: 'username' %><br><br>
    <%= f.text_field :email, placeholder: 'email address' %><br><br>
    <%= f.password_field :password_digest, placeholder: 'password' %><br><br>
    <%= f.submit 'login', id: "accessButton" %>
<% end %>